 pipeline {
     agent any
     stages {
         stage ('clone my code') {
           setps {
               git 'https://github.com/dhumalparth/maven-project'
           }
           }
         stage ('compile my code') {
           setps {
               withMaven(maven : 'LocalMaven') {
                   sh 'mvn compile'
                   }
                   }
                   }
                   }
                   }
         
